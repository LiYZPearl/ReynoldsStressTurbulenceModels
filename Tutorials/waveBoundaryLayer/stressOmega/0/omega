FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2.0;

boundaryField
{
    top
    {
        type            slip;
    }

    bottom
    {
	    type		    omegaWallFunction;
            value uniform 2.0;
    }
    


    frontAndBack
    {
        type            empty;
    }
    
    inlet
    {
	    type		    zeroGradient;
    }
    
    
    outlet
    {
	    type		    zeroGradient;
    }
}

