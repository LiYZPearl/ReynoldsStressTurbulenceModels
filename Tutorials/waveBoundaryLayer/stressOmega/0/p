FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
              type            slip;
	      //	           type            fixedValue;
	      //   value           uniform 0.0;
    }

    bottom
    {
	    type		    zeroGradient;
    }
   


    frontAndBack
    {
        type            empty;
    }
    inlet
    {
       type		    zeroGradient;
       // type            fixedValue;
       //   		   value           uniform 0.0; 
      
    }
    outlet
    {
      	    type		    zeroGradient;
      //type            fixedValue;
      //value           uniform 0;
    }

}

